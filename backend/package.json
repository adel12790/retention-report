    "devDependencies": {
        "@eslint/js": "^9.34.0",
        "@tsconfig/node22": "^22.0.2",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/jest": "^30.0.0",
        "@types/morgan": "^1.9.10",
        "@types/node": "^24.3.0",
        "@types/sqlite3": "^5.1.0",
        "@types/supertest": "^6.0.3",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/eslint-plugin": "^1.3.5",
        "eslint": "^9.34.0",
        "eslint-plugin-perfectionist": "^4.15.0",
